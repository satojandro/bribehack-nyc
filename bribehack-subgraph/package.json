    "create-local": "graph create --node http://localhost:8020/ bribehack/bribehack-subgraph",
    "remove-local": "graph remove --node http://localhost:8020/ bribehack/bribehack-subgraph",
    "deploy-local": "graph deploy --node http://localhost:8020/ --ipfs http://localhost:5001 bribehack/bribehack-subgraph",
    "test": "graph test"
  },
  "dependencies": {
    "@graphprotocol/graph-ts": "0.31.0"
  },
  "devDependencies": {
    "@graphprotocol/graph-cli": "0.60.0",
    "matchstick-as": "0.5.0"
  }
}