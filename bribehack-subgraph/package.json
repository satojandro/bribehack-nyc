{
  "name": "bribehack-subgraph",
  "version": "1.0.0",
  "description": "The Graph subgraph for Bribehack - tracking commitments, bribes, and bounty sponsorships",
  "scripts": {
    "codegen": "graph codegen",
    "build": "graph build",
    "deploy": "graph deploy --node https://api.studio.thegraph.com/deploy bribehack-subgraph",
